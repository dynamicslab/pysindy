{
    "version": 1,
    "branches": ["main"],
    "project": "pysindy",
    "project_url": "https://pysindy.readthedocs.io/",
    "repo": "..",
    "environment_type": "virtualenv",
    "env_dir": ".asv/env",
    "results_dir": ".asv/results",
    "html_dir": ".asv/html",
    "show_commit_url": "https://github.com/$OWNER/$REPO/commit/",
    "build_command": [
        "PIP_NO_BUILD_ISOLATION=0 python -m build",
        "PIP_NO_BUILD_ISOLATION=false python -m pip wheel --no-deps -w {build_cache_dir} {build_dir}"
    ],
    "install_command": [
        "in-dir={env_dir} python -m pip install {wheel_file}[bench] --force-reinstall"
    ]
}
